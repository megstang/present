
<% if current_user %>
  <%= link_to 'Log Out', '/sessions', method: :delete %>
  <h1><%= @inning.name %> Inning</h1>
  <h3>Modules</h3>
    <table>
      <% @inning.turing_modules.each do |mod| %>
        <tr>
          <td><%= link_to mod.name, user_turing_module_path(mod) %></td>
          <td><%= link_to 'Take Attendance for this Module', new_user_turing_module_attendance_path(mod) %></td>
        </tr>
      <% end %>
    </table>
<% else %>
  <%= link_to "Sign In With Google", "/auth/google_oauth2" %> to get started
<% end %>
